<view wx:if="{{toCertify===1}}">

    <view class="pro-list-wrappper">
        <view class="society-name">以下是{{societyName}}的项目列表</view>
        <view wx:if="{{!projects.length}}" class="no-project">您还没有项目</view>
        <view wx:else>
            <view wx:for="{{projects}}">
                <view class="pro-item" wx:key>
                    {{item.i_name}}
                    <view class="delete-wrapper">
                        <text bindtap="deletePro" data-id=" {{item.id}}" data-idx="{{index}}" wx:if="{{item.state===0}}">待审核</text>
                        <view data-id=" {{item.id}}" data-idx="{{index}}" wx:elif="{{item.state===2}}">
                            审核失败
                            <text bindtap="deletePro" data-id=" {{item.id}}" data-idx="{{index}}">删除</text>
                        </view>
                        <text data-id=" {{item.id}}" data-idx="{{index}}" wx:elif="{{item.state===1}}" bindtap="deletePro">删除</text>

                    </view>
                </view>
            </view>

        </view>
    </view>



    <view class="add-project-wrapper">
        <view class="in_tips">
            <b>项目名字</b>
        </view>
        <input data-t="project_title" bindblur="input_project_data" value="{{project_title}}" />
        <view class="in_tips">
            <b>项目预算</b>
        </view>
        <input data-t="project_price" bindblur="input_project_data" type="number" placeholder="￥" value="{{project_price}}" />

        <view class="in_tips">
            <b>项目介绍</b>
            <s>请使用以下文本编辑器填写项目介绍</s>
        </view>
